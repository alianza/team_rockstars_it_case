    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.0.1",
    "workbox-core": "^5.1.3",
    "workbox-expiration": "^5.1.3",
    "workbox-precaching": "^5.1.3",
    "workbox-routing": "^5.1.3",
    "workbox-strategies": "^5.1.3"
  },
  "devDependencies": {
    "@testing-library/cypress": "^8.0.1",
    "@testing-library/jest-dom": "^5.11.4",
